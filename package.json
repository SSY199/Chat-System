{
  "name": "chat-system",
  "version": "1.0.0",
  "description": "A simple chat system using Node.js and Socket.IO",
  "main": "index.js",
  "scripts": {
  "install": "cd server && npm install && cd ../client && npm install",
  "build": "cd client && npm run build",
  "start": "cd server && npm start",
  "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
  "dev:server": "cd server && npm run dev",
  "dev:client": "cd client && npm run dev"
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SSY199/Chat-System.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/SSY199/Chat-System/issues"
  },
  "homepage": "https://github.com/SSY199/Chat-System#readme",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
